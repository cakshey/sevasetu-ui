{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
        { "key": "Content-Encoding", "value": "br" }
      ]
    }
  ],
  "images": {
    "formats": ["image/avif", "image/webp"]
  },
  "cleanUrls": true,
  "trailingSlash": false
}
